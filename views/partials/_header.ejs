<header>
    <div class="panel panel-default">

        <div class="panel-body">

            <div class="row">
                <div  class="col-md-4">
                    <label class="control-label">考试科目：</label>
                    <strong> 统一建模语言 UML </strong>
                </div>
                <div  class="col-md-4">
                    <label class="control-label">时间： </label>
                    <label>100 分钟 </label>
                </div>
                <div  class="col-md-4">
                    <label class="control-label">得分： </label>
                    <label class="text-danger" id="totalScore"><%= totalScore %></label>
                </div>
            </div>

            <div class="row">
                <div class="col-md-4 form-group form-inline">
                    <label class="control-label" for="grade">班 级：</label>
                    <input class="form-control student_info" name="grade" type="text" id="grade"
                           value="<%= studentInfo.grade %>"/>
                </div>
                <div class="col-md-4 form-group form-inline">
                    <label class="control-label" for="studentId">学号：</label>
                    <input class="form-control student_info" name="studentId" type="text" id="studentId"
                           value="<%= studentInfo.studentId %>"/>
                </div>
                <div class="col-md-4 form-group form-inline">
                    <label class="control-label" for="studentName">姓名：</label>
                    <input class="form-control student_info" name="studentName" type="text" id="studentName"
                           value="<%= studentInfo.studentName %>"/>
                </div>
            </div>

        </div>
    </div>
</header>